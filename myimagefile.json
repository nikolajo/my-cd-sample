{
  "taskDefinition": {
    "family": "my-cd-image",
    "containerDefinitions": [
      {
        "name": "my-cd-image",
        "image": "nikolajo/my-cd-image:latest",
        "cpu": 100,
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 80,
            "hostPort": 80
          }
        ],
        "memory": 128,
        "essential": true
      }
    ]
  }
}